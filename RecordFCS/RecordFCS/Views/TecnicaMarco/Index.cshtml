@model IEnumerable<RecordFCS.Models.TecnicaMarco>

@{
    ViewBag.Title = "Técnicas de Marco";
}

<h1>
    <b>CATALOGO</b>
    <i>@ViewBag.Title</i>
</h1>

<ol class="breadcrumb">
    <li>@Html.ActionLink("Home", "Index", "Home")</li>
    <li class="active">@ViewBag.Title</li>
</ol>


<!-- Campo que se reemplaza-->
<div id="replacetarget">
    @{ Html.RenderAction("Lista", "TecnicaMarco"); }
</div>




<!-- modal placeholder-->
<div id='myModal' class='modal fade in' data-backdrop="static">
    <div class="modal-dialog">
        <div class="modal-content">
            <div id='myModalContent'></div>
        </div>
    </div>
</div>


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")

}
